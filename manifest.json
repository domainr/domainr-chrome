{
	"name" : "Domainr"
	, "version" : "4.0.0"
	, "manifest_version" : 2
	, "description"	: "Worldwide domain search."
	, "browser_action": {
	  	"default_icon" : "icon.png"
		, "default_popup" : "domainr.html"
		, "default_title" : "Domainr"
	},
	"icons" : {
	  	"128" : "icon_128.png"
	},
	"permissions": [
	    "http://domainr.com/*"
	  , "https://domainr.com/*"
    , "tabs"
	]
}
